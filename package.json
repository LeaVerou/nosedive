{
  "name": "pathed",
  "version": "0.0.1",
  "description": "Get and set values in nested objects using a path",
  "type": "module",
  "main": "./dist/pathed.cjs",
  "module": "./dist/pathed.js",
  "scripts": {
    "test": "npx htest test/index.js",
    "build:html": "",
    "build:docs": "npx typedoc --out docs src/",
