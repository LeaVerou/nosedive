  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leaverou/pathed/issues"
  },
  "homepage": "https://github.com/leaverou/pathed/#readme",
  "devDependencies": {
    "@stylistic/eslint-plugin": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest",
    "htest.dev": "file:../htest.dev",
    "release-it": "latest",
    "rollup": "^2",
    "typedoc": "latest"
  }
